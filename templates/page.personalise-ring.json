nt;\n    margin: 5px 0;\n}\n\n.custom-liquid-block p {\n    font-family: \"oregon-ldo-medium\";\n    font-size: 16px;\n    font-weight: normal !important; \/* Ensures normal weight *\/\n    background: linear-gradient(to right, #924c26 0%, #ce875e 49%, #924c26 100%);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n    margin: 5px 0;\n}\n\n.custom-liquid-block .book-now {\n    font-weight: bold;\n}\n\n.custom-liquid-block button.show-form {\n    background-color: black;\n    color: white;\n    border: none;\n    padding: 12px 24px;\n    font-size: 16px;\n    font-family: \"oregon-ldo-medium\";\n    cursor: pointer;\n    border-radius: 30px;\n    transition: background 0.3s;\n    width: 80%;\n    max-width: 300px;\n    display: block;\n    margin: 10px auto;\n    height:50px;\n}\n\n.custom-liquid-block button.show-form:hover {\n    background-color: #333;\n}\n<\/style>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n    document.querySelector(\".show-form\").addEventListener(\"click\", function() {\n        document.getElementById(\"form-container\").style.display = \"block\";\n    });\n});\n<\/script>","color_scheme":"","padding_top":40,"padding_bottom":52}},"custom_liquid_j8cXAP":{"type":"custom-liquid","settings":{"custom_liquid":"<div class=\"text-container\">\n    <h1 id=\"engagement-heading\" class=\"styled-text\"><\/h1>\n<\/div>\n\n<style>\n.styled-text {\n    font-family: \"oregon-ldo-medium\", sans-serif;\n    font-size: 28px;\n    font-weight: normal;\n    color: #000000;\n    text-align: center;\n    display: inline-block;\n    max-width: 100%;\n    line-height: 45px;\n    overflow: hidden;\n    white-space: nowrap;\n    opacity: 0;\n    transform: translateX(-100%);\n    transition: opacity 0.8s ease-out, transform 0.8s ease-out;\n}\n\n\/* Gradient only on YES and PLACE *\/\n.gradient-text {\n    font-family: \"oregon-ldo-medium\";\n    background: linear-gradient(to right, #924c26 0%, #ce875e 49%, #924c26 100%);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n}\n\n\/* Larger font for YES and PLACE *\/\n.large-text {\n    font-size: 32px;\n}\n\n\/* Class to trigger wipe animation *\/\n.wipe-in {\n    opacity: 1;\n    transform: translateX(0);\n}\n\n\/* Ensure full background coverage *\/\n.text-container {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    background: #f8f8f8;\n    padding: 40px 0; \/* Ensures no white space *\/\n    margin: 0; \/* Removes extra margins *\/\n    min-height: 100px; \/* Adjust as needed *\/\n}\n<\/style>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n    const textElement = document.getElementById(\"engagement-heading\");\n\n    const textLines = [\n        \"Are You Getting Engaged\",\n        \"In 2025?\",\n        \"If <span class='gradient-text large-text'>YES<\/span>\",\n        \"you are at the right <span class='gradient-text large-text'>PLACE<\/span>.\"\n    ];\n\n    let lineIndex = 0;\n\n    function showLine() {\n        textElement.innerHTML = textLines.slice(0, lineIndex + 1).join(\"<br>\");\n        textElement.classList.add(\"wipe-in\");\n\n        if (lineIndex < textLines.length - 1) {\n            lineIndex++;\n            setTimeout(showLine, 1200); \/\/ Delay before showing next line\n        } else {\n            setTimeout(() => {\n                textElement.classList.remove(\"wipe-in\");\n                setTimeout(() => {\n                    textElement.innerHTML = \"\";\n                    lineIndex = 0;\n                    showLine();\n                }, 1000); \/\/ Pause for 3 seconds before restarting\n            }, 1000); \/\/ Keep final text visible for 3 sec before clearing\n        }\n    }\n\n    showLine();\n});\n<\/script>","color_scheme":"","padding_top":40,"padding_bottom":52}},"rich_text_bRJ66G":{"type":"rich-text","blocks":{"heading_p3QLmm":{"type":"heading","settings":{"headin