details h2 {\n      font-size: 28px; \/* Increased font size for desktop *\/\n      padding: 0; \/* No padding needed for desktop *\/\n    }\n\n    \/* Show the product description on desktop, with !important to ensure visibility *\/\n    .video-product-container .product-details .product-description {\n      display: block !important; \/* Force show on desktop *\/\n      font-size: 18px; \/* Adjusted font size for desktop *\/\n      padding: 0; \/* Remove padding for desktop *\/\n    }\n  }\n<\/style>","color_scheme":"","padding_top":12,"padding_bottom":16}},"rich_text_gX4WJC":{"type":"rich-text","blocks":{"heading_YTDkYC":{"type":"heading","settings":{"heading":"We Offer 9 Price Variants For You","heading_size":"h2"}},"button_RMwYWr":{"type":"button","disabled":true,"settings":{"button_label":"","button_link":"","button_style_secondary":false,"button_label_2":"","button_link_2":"","button_style_secondary_2":false}}},"block_order":["heading_YTDkYC","button_RMwYWr"],"custom_css":["H2 {color: #be868c;}"],"settings":{"desktop_content_position":"center","content_alignment":"center","color_scheme":"","full_width":true,"padding_top":0,"padding_bottom":24}},"custom_liquid_eTcYhR":{"type":"custom-liquid","settings":{"custom_liquid":"<style>\n#goldButton,\n#platinumButton {\n  background-color: black;\n  color: white;\n  border: 2px solid transparent; \/* Assuming you wanted a border, specify style *\/\n  cursor: pointer;\n  width: 150px;\n  height: 50px;\n  border-radius: 10px;\n  margin-top: 5px;\n  margin-right: 5px; \/* Keeps original right margin for mobile *\/\n  margin-left: 22px; \/* Keeps original left margin for mobile *\/\n  font-size: 16px;\n}\n\n#goldButton:hover,\n#platinumButton:hover {\n  opacity: 0.8;\n}\n\nh2 { \n  margin-left: 20px;\n  margin-top: 5px;\n  font-size: 20px;\n}\n\n\/* Media query for desktop devices *\/\n@media screen and (min-width: 1024px) {\n  #goldButton,\n  #platinumButton {\n    width: 200px; \/* Larger for desktop *\/\n    height: 60p